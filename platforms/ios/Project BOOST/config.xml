<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.hood.boost" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="RFduino">
        <param name="ios-package" onload="true" value="MEGRFduino" />
    </feature>
    <feature name="EmailComposer">
        <param name="ios-package" value="APPEmailComposer" />
    </feature>
    <feature name="CallNumber">
        <param name="ios-package" value="CFCallNumber" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="BackupWebStorage" value="local" />
    <icon height="180" src="res/hood180.png" width="180" />
    <icon height="60" src="res/hood60.png" width="60" />
    <icon height="120" src="res/hood120.png" width="120" />
    <icon height="76" src="res/hood76.png" width="76" />
    <icon height="152" src="res/hood152.png" width="152" />
    <icon height="40" src="res/hood40.png" width="40" />
    <icon height="80" src="res/hood80.png" width="80" />
    <icon height="57" src="res/hood57.png" width="57" />
    <icon height="114" src="res/hood114.png" width="114" />
    <icon height="72" src="res/hood72.png" width="72" />
    <icon height="144" src="res/hood144.png" width="144" />
    <icon height="29" src="res/hood29.png" width="29" />
    <icon height="58" src="res/hood58.png" width="58" />
    <icon height="50" src="res/hood50.png" width="50" />
    <icon height="100" src="res/hood100.png" width="100" />
    <name>Project BOOST</name>
    <description>
    An application to interact with Project BOOST temperature data loggers.
  </description>
    <author email="jb38@hood.edu" href="http://cs.hood.edu/">
    Jim Blaney
  </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="default" />
    <preference name="BackgroundColor" value="0xff2e608b" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
